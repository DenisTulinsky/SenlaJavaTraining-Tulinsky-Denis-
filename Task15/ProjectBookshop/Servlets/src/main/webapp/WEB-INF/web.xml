<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>BooksServlet</servlet-name>
		<servlet-class>servlets.BooksServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>OrdersServlet</servlet-name>
		<servlet-class>servlets.OrdersServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>PreordersServlet</servlet-name>
		<servlet-class>servlets.PreordersServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>BooksServlet</servlet-name>
		<url-pattern>/BooksServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>OrdersServlet</servlet-name>
		<url-pattern>/OrdersServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PreordersServlet</servlet-name>
		<url-pattern>/PreordersServlet</url-pattern>
	</servlet-mapping>
</web-app>